{
  
  "darkmode": true,
  "themeLocation": "theme.json",
  "pages": [
    "pages/login/login",
    "pages/home/home",
    "pages/group/notice/notice",
    "pages/group/notice/notice_send/notice_send",
    "pages/group/notice/notice_detail/notice_detail",
    "pages/group/group",
    "pages/me/me",
    "pages/group/add_group/add_group"
  ],
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将用于小程序位置接口的效果展示"
    }
  },
  "window": {

    "navigationBarTitleText": "ColorUI",
    "navigationStyle": "custom",
    "navigationBarBackgroundColor": "@navBgColor",
    "navigationBarTextStyle": "@navTxtStyle",
    "backgroundColor": "@bgColor",
    "backgroundTextStyle": "@bgTxtStyle",
    "backgroundColorTop": "@bgColorTop",
    "backgroundColorBottom": "@bgColorBottom"
  },
  "usingComponents": {
    "cu-custom": "/colorui/components/cu-custom"
  },
  "sitemapLocation": "sitemap.json",
  "tabBar": {
    "color": "#646464",
    "selectedColor": "#000000",
    "backgroundColor": "#e6e6e6",
    "borderStyle": "black",
    "list": [
      {
        "pagePath": "pages/group/group",
        "text": "群组",
        "iconPath": "/images/icon_left.jpg",
        "selectedIconPath": "images/icon_left_selected.jpg"
      },
      
      {
        "pagePath": "pages/me/me",
        "text": "关于",
        "iconPath": "/images/icon_right.jpg",
        "selectedIconPath": "images/icon_right_selected.jpg"
      }
    ]
  },
  "useExtendedLib": {
    "kbone": true,
    "weui": true
  }
}
