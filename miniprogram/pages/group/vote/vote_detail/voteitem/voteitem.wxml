<cu-custom bgImage="{{noticedetail_jpg}}" isBack="{{true}}">
  <view slot="backText">返回</view>
</cu-custom>
<form bindsubmit="formSubmit">
  <view>
    <view style="height: 15px"></view>
  </view>
  <view>标题：{{title}}</view>
  <view>
    <radio-group bindchange="radioChange">
      <label class="weui-cell weui-check__label" wx:for="{{items}}">
        <view class="weui-cell__hd">
          <radio value="{{item.value}}" checked="true"/>
        </view>
        <view class="weui-cell__bd">{{item.name}}</view>
      </label>
    </radio-group>
  </view>
  <view>
    <button form-type="submit">投票</button>
  </view>
</form>


<view wx:if="{{userType ===  1}}">
    <view class="text-grey" wx:for="{{namelist}}">{{item}} {{received[item]}} </view>
</view>